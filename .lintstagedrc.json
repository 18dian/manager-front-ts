{
  "*.{js,jsx,ts,tsx}": ["prettier --write", "eslint --fix"],
  "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": ["prettier --write--parser json"],
  "package.json": ["prettier --write"],
  "*.vue": ["prettier --write", "eslint --fix"],
  "*.{sass,less,styl,html}": ["prettier --write"]
}